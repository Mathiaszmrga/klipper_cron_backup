	{% set bedtemp = params.BED|int %}
	{% set hotendtemp = params.HOTEND|int %}
	{% set filament_profile = params.FILID|string %}
	G21
	G90
	M83
	G28
	NEOPIXEL_DISPLAY LED=underglow TYPE=extruder_temp MODE=glow
